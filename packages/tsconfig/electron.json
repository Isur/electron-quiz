{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Electron.js",
    "extends": "./base.json",
    "compilerOptions": {
        "allowJs": true,
        "module": "commonjs",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react"
    },
    "include": ["src", "next-env.d.ts"],
    "exclude": ["node_modules"]
  }
  